    <br>
    <form action="/formulario" method="post">
        <div class="form-group">
            <label for="producto"><b>Produto</b></label>
            <input id="producto" class="form-control" type="text" name="producto">
        </div>
        <div class="form-group">
            <label for="precio"><b>Precio</b></label>
            <input id="precio" class="form-control" type="number" name="precio">
        </div>
        <div class="form-group">
            <label for="img"><b>Imagen</b></label>
            <input id="img" class="form-control" type="text" name="img">
